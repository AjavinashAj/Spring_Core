package in.ashokit;

public class Student {
	private int sid;
	private String sname;
	private Double marks;
	public Student(int sid, String sname, Double marks) {
		super();
		this.sid = sid;
		this.sname = sname;
		marks = marks;
	}
	
   public String rank()
   {
	   String st;
	   if(marks>=80)
	   {
		   st="Grade A";
	   }
	   else if(marks>60 && marks<80)
	   {
		   st="Grade B";
	   }
	   else if(marks>45 && marks<50)
	   {
		   st ="Grade C";
	   }
	   else if(marks>35 && marks<45)
	   {
		   st="Grade D";
	   }
	   else
	   {
		   st="Grade E";
	   }
	   return st;
   }
}
